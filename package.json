{
	"name": "zotero-citation-picker",
	"displayName": "Zotero Citation Picker",
	"description": "Zotero Better Bibtex citation picker for VS Code",
	"version": "0.1.1",
	"publisher": "mblode",
	"author": {
		"name": "Matthew Blode",
		"email": "m.blode@gmail.com",
		"url": "https://matthewblode.com"
	},
	"homepage": "https://github.com/mblode/vscode-zotero",
	"bugs": {
		"url": "https://github.com/mblode/vscode-zotero/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/mblode/vscode-zotero.git"
	},
	"icon": "images/icon.png",
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"markdown",
		"zotero",
		"betterbibtex",
		"better bibtex",
		"citations"
	],
	"activationEvents": [
		"onCommand:extension.zoteroCitationPicker"
	],
	"main": "./src/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.zoteroCitationPicker",
				"title": "Zotero Citation Picker"
			}
		],
		"keybindings": [
			{
				"command": "extension.zoteroCitationPicker",
				"key": "alt+shift+z"
			}
		]
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^8.10.25",
		"eslint": "^4.11.0",
		"typescript": "^3.1.4",
		"vscode": "^1.1.25"
	},
	"dependencies": {
		"request-promise": "^4.2.2"
	}
}
